{
  "name": "nytreact",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client": "cd client && yarn start",
    "start": "node ./bin/www",
    "watch": "PORT=3001 nodemon bin/www",
    "heroku-postbuild": "cd client && yarn && yarn run build",
    "dev": "concurrently --kill-others-on-fail \"yarn watch\" \"yarn client\" "
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongoose": "^5.2.4",
    "morgan": "~1.9.0"
  }
}
